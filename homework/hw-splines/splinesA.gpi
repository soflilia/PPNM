set terminal svg enhanced font 'Arial,12'
set terminal svg background "white"

set output "splinesA.svg"
set xlabel "X" 
set ylabel "Y"
set grid
set title "Linear spline interpolation of cos(x)"
set yrange[-1.2 to 1.2]


f(u) = cos(u)


plot\
    "<paste output_xs.txt output_splines.txt" title "spline" ,\
    "<paste output_xs.txt output_splinesint.txt" title "anti derivative"